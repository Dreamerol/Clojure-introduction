                             (->>
                              (assoc {} "body" (get item "comment"))
                              (conj acc))
                             ;; (prn (assoc {} "body" (get item "comment")))
                             )
                           [] els)]


     
     (->
      (first els)
      (dissoc  "comment")
      (assoc  "comments" comments)
      ))

