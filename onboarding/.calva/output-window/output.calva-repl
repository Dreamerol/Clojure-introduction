(defn grouping [key els]
  (let [unique-keys (reduce (fn [acc item]
                              (->> 
                               (get item key)
                               (conj acc)))
                            #{} els)]
  (reduce (fn [acc item]
            (conj acc [item (get-items-behind-val els key item)]))
          [] unique-keys))
    )
